        <activity android:name="com.ishaanbhela.ishaanstictactoe.Results" />
        <activity android:name="com.ishaanbhela.ishaanstictactoe.TicTacToe" />
        <activity android:name="com.ishaanbhela.ishaanstictactoe.splash_scr">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
