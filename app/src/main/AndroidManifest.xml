        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">
        <activity android:name="com.ishaanbhela.ishaanstictactoe.MainActivity"></activity>
        <activity android:name="com.ishaanbhela.ishaanstictactoe.ResultsMultiPlayer" />
